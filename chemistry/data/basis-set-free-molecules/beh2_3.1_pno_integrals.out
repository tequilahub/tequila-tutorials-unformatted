         econv_pairs  1.0000e-05 # derived   ENergy convergence for individual pairs in adaptive solver. Converged pairs are frozen automatically
         econv_macro  1.0000e-04 # derived   Energy convergence for macroiterations in adaptive solver, no effect if adaptive_solver is deactivated
               dconv  1.0000e-01 # default   convergence of every PNO in the Green's function solver
           op_thresh  1.0000e-06 # default   MRA operator thresh
             restart  none       # derived   restart pairs of this type, use 'mp2', 'cispd', 'all' or 'none'
          no_compute  none       # default   do not compute the pairs of this type, use 'mp2', 'cispd', 'all' or 'none'
              no_opt  none       # default   do not optimize the pnos of this type, use 'mp2', 'cispd', 'all' or 'none'
            no_guess  none       # derived   guess for this type will be empty, use 'mp2', 'cispd', 'all' or 'none'
     adaptive_solver  all        # default   Use adaptive solver for those pairs, use 'mp2', 'cispd', 'all' or 'none', works only in combination with guesstype 'exop'
                kain  true       # default   use KAIN solver in amplitude solver
       kain_subspace  5          # default   subspace size of the KAIN solver (amplitudes)
                 f12  false      # defined   use explicit correlation
        cispd_number  -1         # default   CIS(D) excitation numbers, to read in correct functions
        cispd_energy  0.0000e+00 # default   CIS energy
        freeze_pairs  none       # default   frozen pairs will not be optimized: Expected format 'a b c d' will freeze pairs ab and cd
freeze_pairs_of_orbital  []         # default    All pairs which originate from this orbital will not be optimized
active_pairs_of_orbital  []         # default    All pairs which originate from this orbital will not be frozen all other pairs will, if this vector is not empty
no_opt_in_first_iteration  false      # default   Do not optimize in the first iteration (then the potentials do not have to be evaluated, use this for large guesses)
